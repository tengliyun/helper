{
    "name": "tengliyun/helper",
    "description": "The Tengliyun Components",
    "keywords": [
        "Tengliyun Helper",
        "Tengliyun Utils"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/tengliyun/helper",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tengliyun\\Helper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
